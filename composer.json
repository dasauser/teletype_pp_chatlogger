{
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.0",
    "yiisoft/yii2": "2.0.50",
    "psr/log": "~3.0",
    "monolog/monolog": "~3.0",
    "yiisoft/yii2-httpclient": "~2.0.15"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
